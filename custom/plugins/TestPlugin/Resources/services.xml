<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
      <service id="test_plugin.subscriber.templates" class="TestPlugin\Subscriber\Frontend">
            <argument>%test_plugin.plugin_dir%</argument>
            <argument type="service" id="service_container"/>
            <tag name="shopware.event_subscriber"/>

        </service>

  <!-- <service id="test_plugin.subscriber.templatess" class="TestPlugin\Subscriber\Backend">
            <argument>%test_plugin.plugin_dir%</argument>
       <argument type="service" id="service_container"/>
             <tag name="shopware.event_subscriber"/>
        </service>
-->
            <service id="test_plugin.test_service" class="TestPlugin\Components\Api\Resource\TestService" >
        <argument type="service" id="service_container"/>
    </service>

        <service id="shopware.api.bundle" class="TestPlugin\Components\Api\Resource\Bundle"/>

        <service class="TestPlugin\SearchBundleDBAL\CriteriaRequestHandler" id="test_plugin.search_bundle_dbal.criteria_request_handler">
            <tag name="criteria_request_handler"/>
        </service>

        <service class="TestPlugin\SearchBundleDBAL\Condition\GadgetConditionHandler" id="test_plugin.search_bundle_dbal_condition.gadget_condition_handler">
            <tag name="condition_handler_dbal"/>
        </service>

        <service class="TestPlugin\SearchBundleDBAL\Facet\GadgetFacetHandler" id="test_plugin.search_bundle_dbal_facet.gadget_facet_handler">
            <tag name="facet_handler_dbal"/>
            <argument type="service" id="shopware_searchdbal.dbal_query_builder_factory"/>
            <argument type="service" id="snippets"/>
        </service>

    </services>
</container>